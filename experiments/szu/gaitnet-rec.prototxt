}

# block 3 -------------------------------------------------------------------------------------
layer {
  name: "conv3_0"
  type: "Convolution"
  bottom: "pool2_0"
  top: "conv3_0"
  param {
    lr_mult: 0
    #decay_mult: 0
  }
  param {
    lr_mult: 0
    #decay_mult: 0
  }
