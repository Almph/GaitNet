name: "GaitNet-rec"

layer {
  name: "data"
  type: "Python"
  top: "im0"
  top: "im1"
  top: "im2"
  top: "im3"
  top: "im4"
  top: "im5"
  top: "gt0"
  top: "gt1"
  top: "gt2"
  top: "gt3"
  top: "gt4"
  top: "gt5"
  top:"label"
  python_param {
    module: 'layers'
    layer: 'GaitNet'
    param_str: "{'batch_size': 128,'train_num': 49,'im_path':'/home/user1/data/SZU/crop-im/','gt_path':'/home/user1/data/SZU/crop-seg/','mode':'TRAIN'}"
  }
  include{
    phase:TRAIN
  }
}

layer {
  name: "data"
  type: "Python"
  top: "im0"
  top: "im1"
  top: "im2"
  top: "im3"
  top: "im4"
  top: "im5"
  top: "gt0"
  top: "gt1"
  top: "gt2"
  top: "gt3"
  top: "gt4"
  top: "gt5"
  top:"label"
  python_param {
    module: 'layers'
    layer: 'GaitNet'
    param_str: "{'batch_size': 10,'train_num': 49,'im_path':'/home/user1/data/SZU/crop-im/','gt_path':'/home/user1/data/SZU/crop-seg/','mode':'TEST'}"
  }
  include{
    phase:TEST
  }
}

layer {
  name: "im_cube"
  type: "Concat"
  bottom: "im0"
  bottom: "im1"
  bottom: "im2"
  bottom: "im3"
  bottom: "im4"
  bottom: "im5"
  top: "im_cube"
  concat_param {
    axis: 0
  }
}

layer {
  name: "gt_cube"
  type: "Concat"
  bottom: "gt0"
  bottom: "gt1"
  bottom: "gt2"
  bottom: "gt3"
  bottom: "gt4"
  bottom: "gt5"
  top: "gt_cube"
  concat_param {
    axis: 0
  }
}
# block 1 ----------------------------------------------------------------------------------------
layer {
  name: "conv1_0"
  type: "Convolution"
  bottom: "im_cube"
  top: "conv1_0"
  param {
    lr_mult: 0
    #decay_mult: 0
  }
  param {
    lr_mult: 0
    #decay_mult: 0
  }
  convolution_param {
    num_output: 48
    kernel_size: 5
    pad: 0
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
    
  }
}
layer {
  name: "relu1_0"
  type: "ReLU"
  bottom: "conv1_0"
  top: "conv1_0"
}
layer {
  name: "pool1_0"
  type: "Pooling"
  bottom: "conv1_0"
  top: "pool1_0"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2    
  }
}

# block 2 ----------------------------------------------------------------------------------------
layer {
