  top: "conv8_0"
}

###############  Seg Loss    #####################
layer {
  name: "loss_seg"
  type: "EuclideanLoss"
  bottom: "gt_cube"
  bottom: "conv8_0"
  top: "loss_seg"
  loss_weight: 0
}


##############  Gait  Rec    ####################
layer {
  name: "make_pair"
  type: "Slice"
  bottom: "conv8_0"
  top: "mask0"
  top: "mask1"
  top: "mask2"
  top: "mask3"
  top: "mask4"
  top: "mask5"
  slice_param {
    axis: 0
    slice_point: 128
    slice_point: 256
    slice_point: 384
    slice_point: 512
    slice_point: 640
  }
    include {
    phase: TRAIN
  }
}
layer {
  name: "make_pair"
  type: "Slice"
  bottom: "conv8_0"
  top: "mask0"
  top: "mask1"
  top: "mask2"
  top: "mask3"
  top: "mask4"
  top: "mask5"
  slice_param {
    axis: 0
    slice_point: 10
    slice_point: 20
    slice_point: 30
    slice_point: 40
    slice_point: 50
  }
